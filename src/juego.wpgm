import wollok.game.*
import escenario.*
import background.*
import objetosMoviles.*
import niveles.*
import rana.*
import config.*

program juego {
	game.title("Juego Base")
	game.cellSize(10)
	game.height(background.limite_y())
	game.width(background.limite_x() + (background.tamanio_celda() / 2))
	
	config.nivelActual(new Nivel1())
	config.nivelActual().iniciar()
	config.iniciar()
	
	game.start()
	
}