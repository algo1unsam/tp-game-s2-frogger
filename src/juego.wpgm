import wollok.game.*
import config.*
import menuPrincipal.*
import background.*

program juego {
	game.title("Frogger")
	game.cellSize(10)
	game.height(background.limite_y())
	game.width(background.limite_x() + (background.tamanio_celda() / 2))
	config.configTeclas()
	menuPrincipal.mostrar()
	game.start()
	
}